         <Route path='/' exact component={Home} />\n            <ProtectedRoute path='/donor-list' component={DonorList} />\n            <ProtectedRoute path='/create-donor' component={CreateDonor} />\n            <ProtectedRoute\n              path='/donor-details/:id'\n              component={DonorDetails}\n            />\n            {/* <ProtectedRoute path = '/add-donation/:id' component={AddDonation}/> */}\n            <ProtectedRoute path='/edit-donor/:id' component={EditDonor} />\n            <ProtectedRoute path='/delete-donor/:id' component={DeleteDonor} />\n          </Switch>\n        </div>\n        <Loader promiseTracker={usePromiseTracker} />\n      </div>\n    </Router>\n  );\n};\n\nexport default App;\n","/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/reducers/index.js",[],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Header.js",["156"],"/* \nThis component renders a header that will persist throughout app usage. \nThe login button will dynamically render based on user login status.\n */\n\nimport React from \"react\";\nimport { useAuth0 } from \"@auth0/auth0-react\";\n\n//Components\nimport LoginButton from \"./Auth/LoginButton\";\nimport LogoutButton from \"./Auth/LogoutButton\";\nimport Navbar from \"react-bootstrap/Navbar\";\nimport Spinner from \"react-bootstrap/Spinner\";\n\n// Assets\nimport logo from \"../images/logo.svg\";\nimport UserProfile from \"../components/Auth/UserProfile\";\n\n// Styles\nimport \"../css/header.css\";\n\nconst Header = () => {\n  const { isAuthenticated, isLoading } = useAuth0();\n\n  // Determine whether to show sign in or sign out button.\n  const renderButton = () => {\n    //If loading show empty div\n    if (isLoading) {\n      return (\n        <Spinner animation='border' variant='light' role='login status'>\n          <span className='sr-only'>Loading Login Status</span>\n        </Spinner>\n      );\n    }\n    //After load show button.\n    return isAuthenticated ? <LogoutButton /> : <LoginButton />;\n  };\n\n  const renderUserImage = () => {\n    return isAuthenticated ? <UserProfile /> : <div></div>;\n  };\n\n  return (\n    <Navbar className='navbar bg-dark navbar-dark header' expand='lg'>\n      <div className='header-content'>\n        <div className='header-branding'>\n          <div className='logo-container'>\n            {/* <img src={logo} className='logo' alt='Literacy Council Logo' /> */}\n            <i className=\"fas fa-book-reader text-light logo\"></i>\n\n          </div>\n\n          <h1 className='header-title text-light'>\n            The Literacy Council\n            <br></br>\n            <small className='text-secondary'>Donor Management Portal</small>\n          </h1>\n        </div>\n        <div className='auth-container'>\n          {renderUserImage()}\n          {renderButton()}\n        </div>\n      </div>\n    </Navbar>\n  );\n};\n\nexport default Header;\n","/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Home.js",[],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Auth/ProtectedRoute.js",[],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Donors/index.js",[],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/reducers/donorReducer.js",[],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Auth/LogoutButton.js",[],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Auth/UserProfile.js",[],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Auth/LoginButton.js",[],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Donors/DeleteDonor.js",[],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Donors/DonorDetails.js",["157"],"/* \nThis class component renders a detailed view of a donor, including contact information\nand a record of past donations including amount and type. \n*/\n\nimport React from \"react\";\nimport { connect } from \"react-redux\";\nimport { Link } from \"react-router-dom\";\n\n//Actions\nimport {\n  fetchDonorDetails,\n  showModal,\n  hideModal,\n} from \"../../actions/index.js\";\n\n//Components\nimport ModalRoot from \"../Modal/ModalRoot\";\nimport Button from \"react-bootstrap/Button\";\nimport Table from \"react-bootstrap/Table\";\n\n//Styles\nimport \"../../css/donor-details.css\";\n\n//Helper functions\nimport formatAmount from \"../../helper-funcs/formatAmount\";\nimport formatDate from \"../../helper-funcs/formatDate\";\n\nclass DonorDetails extends React.Component {\n  constructor(props) {\n    super(props);\n  }\n\n  componentDidMount() {\n    this.props.fetchDonorDetails();\n  }\n\n  //Only show notes field if notes exist\n  renderNotes = () => {\n    if (this.props.donor.notes !== \"none\") {\n      return (\n        <div className='donor-detail-notes'>\n          <h2 className='donor-details-section-heading'>Notes</h2>\n          <p className='text-dark'>{`${this.props.donor.notes}`}</p>\n          <hr></hr>\n        </div>\n      );\n    } else {\n      return <div></div>;\n    }\n  };\n\n  renderDonationHistory = () => {\n    return this.props.donor.donations.map((donation, idx) => {\n      //Don't forget to format donation amounts.\n      return (\n        <tr key={idx}>\n          <td className='text-dark'>{formatDate(donation.date)}</td>\n          <td className='text-dark'>{formatAmount(donation.amount)}</td>\n          <td className='text-dark'> {donation.type}</td>\n        </tr>\n      );\n    });\n  };\n\n  closeModal = () => {\n    this.props.hideModal();\n  };\n\n  openDeleteModal = () => {\n    this.props.showModal(\n      {\n        open: true,\n        donor: this.props.donor,\n        closeModal: this.closeModal,\n      },\n      \"delete donor\"\n    );\n  };\n\n  openAddDonationModal = () => {\n    this.props.showModal(\n      {\n        open: true,\n        // title: \"Add Donation\",\n        // message: \"Enter Donor Information\",\n        closeModal: this.closeModal,\n      },\n      \"add donation\"\n    );\n  };\n\n  render() {\n    //Avoids error on refreshing page.\n    if (!this.props.donor) {\n      return <div></div>;\n    }\n\n    return (\n      <div className='donor-details'>\n        <div className='donor-details-wrapper'>\n          <div className='donor-details-container'>\n            <div className='donor-details-heading'>\n              <h1 className='donor-details-title'>Donor Details</h1>\n              <div className='donor-details-action-buttons'>\n                <Link to='/donor-list' className='donor-details-back-btn'>\n                  <Button variant='info'>Back</Button>\n                </Link>\n\n                <Link\n                  to={`/edit-donor/${this.props.match.params.id}`}\n                  className='donor-details-edit-btn'>\n                  <Button variant='success'>Edit</Button>\n                </Link>\n                {/* <Link\n                  to={`/delete-donor/${this.props.match.params.id}`}\n                  className='donor-details-delete-btn'> */}\n                <Button variant='danger' onClick = {this.openDeleteModal}>Delete</Button>\n                {/* </Link> */}\n              </div>\n            </div>\n            <div className='donor-details-contents'>\n              <h2 className='donor-details-section-heading'>Contact Info</h2>\n              <hr></hr>\n\n              <div className='donor-details-contact'>\n                <div className='contact-block-left'>\n                  <div className='donor-detail-name'>\n                    <h4>Name</h4>\n                    <p className='text-dark'>{`${this.props.donor.firstName} ${this.props.donor.lastName}`}</p>\n                  </div>\n                  <div className='donor-detail-organization'>\n                    <h4>Organization</h4>\n                    <p className='text-dark'>{`${this.props.donor.organization}`}</p>\n                  </div>\n                </div>\n\n                <div className='contact-block-center'>\n                  <div className='donor-detail-email'>\n                    <h4>Email</h4>\n                    {/* <a href = 'mailto:' */}\n                    {/* <p className = \"text-dark\">{`${this.props.donor.email}`}</p> */}\n                    <a href={`mailto:${this.props.donor.email}`} target='blank'>\n                      <p className='text-dark'>{`${this.props.donor.email}`}</p>\n                    </a>\n                  </div>\n\n                  <div className='donor-detail-phone'>\n                    <h4>Phone</h4>\n                    <p className='text-dark'>{`${this.props.donor.phone}`}</p>\n                  </div>\n                </div>\n\n                <div className='contact-block-right'>\n                  <div className='donor-detail-address'>\n                    <h4>Address</h4>\n                    <p className='text-dark'>\n                      {`${this.props.donor.street}`}\n                      <br></br>\n                      {`${this.props.donor.city}`},{\" \"}\n                      {`${this.props.donor.state}`} <br></br>\n                      {`${this.props.donor.zip}`}\n                    </p>\n                  </div>\n                </div>\n              </div>\n              <hr></hr>\n              {this.renderNotes()}\n\n              <div className='donor-detail-donations'>\n                <div className='donor-detail-donation-heading-container'>\n                  <h2 className='donor-details-section-heading'>Donations</h2>\n                  <h5>\n                    Total Donations:{\" \"}\n                    {formatAmount(this.props.donor.totalDonations)}\n                  </h5>\n                </div>\n\n                <div className='donor-history-table'>\n                  <Table striped responsive bordered text='center'>\n                    <thead>\n                      <tr className='table-primary'>\n                        <th>Date</th>\n                        <th>Amount</th>\n                        <th>Type</th>\n                      </tr>\n                    </thead>\n                    <tbody>{this.renderDonationHistory()}</tbody>\n                  </Table>\n                  <div className='donor-details-add-donation-button-container'>\n                    <Button\n                      variant='secondary'\n                      size='sm'\n                      className='donor-details-add-donation-button'\n                      onClick={this.openAddDonationModal}>\n                      + Add Donation\n                    </Button>\n                  </div>\n                </div>\n              </div>\n            </div>\n          </div>\n        </div>\n        <ModalRoot hideModal={this.props.hideModal} />\n      </div>\n    );\n  }\n}\n\nconst mapDispatchToProps = (dispatch, ownProps) => ({\n  hideModal: () => dispatch(hideModal()),\n  showModal: (modalProps, modalType) => {\n    dispatch(showModal({ modalProps, modalType }));\n  },\n  fetchDonorDetails: () =>\n    dispatch(fetchDonorDetails(ownProps.match.params.id)),\n});\n\nconst mapStateToProps = (state, ownProps) => {\n  return { donor: state.donors[ownProps.match.params.id] };\n};\n\nexport default connect(mapStateToProps, mapDispatchToProps)(DonorDetails);\n","/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Donors/DonorList.js",["158"],"/* \nThis component renders the main view of the app - a list of donors and their basic\ncontact information, along with action choices for each entry.\n*/\n\nimport { React, useEffect, useState } from \"react\";\nimport { Link } from \"react-router-dom\";\n\n//Hooks\nimport { useDispatch, useSelector } from \"react-redux\";\n\n//Actions\nimport { fetchDonorList } from \"../../actions\";\n\n//Components\nimport Table from \"react-bootstrap/Table\";\nimport Button from \"react-bootstrap/Button\";\nimport ButtonGroup from \"react-bootstrap/ButtonGroup\";\nimport InputGroup from \"react-bootstrap/InputGroup\";\nimport Form from \"react-bootstrap/Form\";\nimport Dropdown from \"react-bootstrap/DropdownButton\";\n// import Loader from 'react-promise-loader';\n// import { usePromiseTracker } from 'react-promise-tracker';\n\n//Helper functions\nimport formatAmount from \"../../helper-funcs/formatAmount\";\nimport sortDonors from \"../../helper-funcs/sortDonors\";\n\n// Styles\nimport \"../../css/donor-list.css\";\n\nconst DonorList = () => {\n  const [sortType, setSortType] = useState('alphabet');\n\n  const list = useSelector(state => state.donors);\n  const dispatch = useDispatch();\n\n  //MAKE A CALL FOR LIST OF DONORS\n  useEffect(() => {\n    dispatch(fetchDonorList());\n  }, [dispatch]);\n\n  //Renders action buttons.\n  const renderActions = id => {\n    return (\n      <div className='donor-list-action-buttons'>\n        <ButtonGroup size='sm'>\n          <Link to={`/donor-details/${id}`}>\n            <Button variant='info' size='sm'>\n              Details\n            </Button>\n          </Link>\n          <Link to={`/edit-donor/${id}`} className='mx-2'>\n            <Button variant='success' size='sm'>\n              Edit\n            </Button>\n          </Link>\n          <Link to={`/delete-donor/${id}`}>\n            <Button variant='danger' size='sm'>\n              Delete\n            </Button>\n          </Link>\n        </ButtonGroup>\n      </div>\n    );\n  };\n\n  const onSort = e => {\n    setSortType(e.target.value);\n    return;\n  };\n\n  //Render the list of donors.\n  const renderList = () => {\n    const newList = Object.values(list);\n\n    //Sort list of donors by either alphabetcial or totalDonations descending\n    //Default to alphabetcial\n    sortDonors(newList, sortType);\n\n    return newList.map((donor, idx) => {\n      return (\n        <tr key={donor.id}>\n          <td className='text-dark'>\n            {donor.firstName} {donor.lastName}\n          </td>\n          <td className = 'text-dark'>{formatAmount(donor.totalDonations)}</td>\n          <td>\n            <a\n              href={`mailto:${donor.email}`}\n              target='blank'\n              className = 'text-muted'>\n              {donor.email}\n            </a>\n          </td>\n          <td className='text-dark text-nowrap'>{donor.phone}</td>\n          <td>{renderActions(donor.id)}</td>\n        </tr>\n      );\n    });\n  };\n\n  return (\n    <div className='donor-list'>\n      <div className='donor-list-wrapper'>\n        <div className='donor-list-container'>\n          <h1 className='display-5 donor-list-title'>Donor History</h1>\n          <div className='donor-list-global-actions'>\n            <Link to='/create-donor'>\n              <Button variant='dark' size='sm' className='add-donor-button'>\n                + Add New Donor\n              </Button>\n            </Link>\n\n            <div className='donation-list-sort-search'>\n              <InputGroup className='donation-list-sort'>\n                <Form.Control size='sm' as='select' onChange={onSort}>\n                  <option defaultValue>Sort Donors</option>\n                  <option value='alphabet'>Alphabetized</option>\n                  <option value='totalDonations'>Total Donations</option>\n                </Form.Control>\n              </InputGroup>\n\n              <InputGroup className='donation-list-search'>\n                <Form.Control\n                  placeholder='Search Coming Soon'\n                  aria-label='Search Donor'\n                  aria-describedby='basic-addon2'\n                  size='sm'\n                  className='donor-search-field'\n                />\n                <InputGroup.Append>\n                  <Button\n                    variant='dark'\n                    size='sm'\n                    className='search-donor-button'>\n                    Search\n                  </Button>\n                </InputGroup.Append>\n              </InputGroup>\n            </div>\n          </div>\n\n          <div className='donor-list-table border border-info rounded'>\n            <Table striped responsive text='center' className = 'rounded'>\n              <thead className='rounded'>\n                <tr className='table-primary table-heading'>\n                  <th>Name</th>\n                  <th>Total Donations</th>\n                  <th>Email</th>\n                  <th>Phone</th>\n                  <th>Actions</th>\n                </tr>\n              </thead>\n              <tbody>{renderList()}</tbody>\n              {/* <Loader promiseTracker={usePromiseTracker}> Loading </Loader> */}\n            </Table>\n          </div>\n        </div>\n      </div>\n    </div>\n  );\n};\n\nexport default DonorList;\n","/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Donors/CreateDonor.js",[],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Donors/EditDonor.js",["159","160"],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/actions/index.js",[],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/actions/types.js",[],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Button.js",[],["161","162"],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/components/Donors/DonorForm.js",[],"/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/helper-funcs/redirect.js",["163"],"//Helper function to redirect user to DonorList after completing an action.\n\n\n\nimport {withRouter} from \"react-router-dom\";\n\n//Redirects to DonorList after action is completed.\nconst redirectToList = (props) => {\n  const { history } = props;\n  if (history) history.push(\"/donor-list\");\n};\n\nexport default redirectToList;","/Users/caseypitman/Desktop/files/personal-projects/donor-tracker/src/helper-f